public class SwapNumbers {
  public static void main(string[] ars){
      int num1 =5;
      int num2 =10;
   system.out.println("Before swapping :num1 = "+ num1 + ",num2="+num2")
      int temp =num1;
           num1 = num2;
            num2 = temp;
     system.out.println("After swapping: num1="+num1+",num2="+num2);

}
}




